<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Respuesta_menu">

    <androidx.appcompat.widget.LinearLayoutCompat
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <include
            android:id="@+id/tbToolbar5"
            layout="@layout/toolbar">

        </include>

        <TextView
            android:layout_below="@+id/tbToolbar5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/tvRespuestaMenutv"
            android:id="@+id/tvRespuestamenu"
            android:layout_marginTop="@dimen/tvRespuestaMenu"
            android:textSize="30dp"
            android:textStyle="bold"/>

        <TextView
            android:id="@+id/tvRespuestamenu22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="pedro"
            android:textSize="30dp"
            android:textStyle="bold"
            android:layout_marginTop="@dimen/tvRespuestamenuMarginTop"/>

        <ImageView
            android:id="@+id/imgCheck"
            android:layout_width="200dp"
            android:layout_height="200dp"
            android:src="@drawable/check"
            android:layout_marginTop="40dp"
            android:layout_gravity="center_horizontal"/>

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="80dp"
            android:text="@string/etButton"
            android:id="@+id/etButton"/>
    </androidx.appcompat.widget.LinearLayoutCompat>

</androidx.constraintlayout.widget.ConstraintLayout>